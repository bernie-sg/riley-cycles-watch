# askSlim Integration Configuration
# Copy this file to .env and fill in your actual credentials
# WARNING: Never commit .env to git - it contains sensitive credentials

# Your askSlim member account credentials
ASKSLIM_USERNAME=your_email@example.com
ASKSLIM_PASSWORD=your_password_here

# askSlim base URL (default: https://askslim.com)
ASKSLIM_BASE_URL=https://askslim.com

# Session storage state path (optional, defaults to src/riley/modules/askslim/storage_state.json)
# ASKSLIM_STORAGE_STATE_PATH=./src/riley/modules/askslim/storage_state.json

# Run browser in headless mode (default: false for initial login, true for automated runs)
ASKSLIM_HEADLESS=false
